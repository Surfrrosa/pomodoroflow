{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "bundleIdentifier": "com.pomodoroflow.dev"
      },
      "android": {
        "package": "com.pomodoroflow.dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "bundleIdentifier": "com.pomodoroflow.preview"
      },
      "android": {
        "package": "com.pomodoroflow.preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production"
      },
      "ios": {
        "bundleIdentifier": "com.surfrrosa.pomodoroflow"
      },
      "android": {
        "package": "com.pomodoroflow.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
